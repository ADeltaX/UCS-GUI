<UserControl x:Class="UCS.UI.UC.Menu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UCS.UI.UC"
             mc:Ignorable="d" 
             d:DesignHeight="36" d:DesignWidth="200" FontSize="16">
    <Grid HorizontalAlignment="Center" Name="ACAB">
        <Image x:Name="Icon" HorizontalAlignment="Left" Height="36" VerticalAlignment="Top" Width="36">
        </Image>
        <Label x:Name="Name" Content="Name" VerticalAlignment="Top" Margin="36,0,5,0" FontFamily="Segoe UI Light" Foreground="White" FontSize="18.667"/>
    </Grid>
</UserControl>
