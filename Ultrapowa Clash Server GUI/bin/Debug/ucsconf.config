<appSettings>
  <add key="startingLevel" value="1" /><!-- User start Level (Beware,9 is the highest starting level) -->
  <add key="startingExperience" value="0" /><!-- Set to random for random - User start experience -->
  <add key="startingGems" value="777500" /><!-- Set to random for random - Beware, very high value will crash UCS -->
  <add key="startingGold" value="100000000" /><!-- Set to random for random - Beware, very high value will crash UCS -->
  <add key="startingElixir" value="500000000" /><!-- Set to random for random - Beware, very high value will crash UCS -->
  <add key="startingDarkElixir" value="10000" /><!-- Set to random for random - Beware, very high value will crash UCS -->
  <add key="startingTrophies" value="1200" /><!--  Set to random for random - Initial tropies (Please Set Below Then 2399, This Because Users Will Know Server Team By Badge-->
  <add key="startingShieldTime" value="0" /><!-- Initial shield duration in seconds -->
  <add key="clientVersion" value="7.200" /><!-- Major version dot Client build -->
  <add key="useCustomPatch" value="false" /><!-- Specify if the server is using patches (.csv, .sc, ...) -->
  <add key="patchingServer" value="https://www.flamewall.net/ucs/patch/" /><!-- Don't forget the trailing slash -->
  <add key="maintenanceMode" value="false" /><!-- People trying to connect will see a maintenance message if set to true -->
  <add key="maintenanceTimeleft" value="1200" /><!-- Please set the time in seconds like 1200 seconds for 20 minutes -->
  <add key="databaseConnectionName" value="sqliteEntities" /><!-- Chose ucsdbEntities for MySql, and edit ucsdbEntities connection string in ucs.exe.config -->
  <add key="loggingLevel" value="1" /><!-- Enable logging for debugging, set value to 1 for (almost) no log -->
  <add key="apiManager" value="true" /><!-- Enable Api Manager for debugging, set value to false for disable -->
  <add key="oldClientVersion" value="http://ultrapowa.com/" /><!-- Place to redirect old version client and don't forget the trailing slash -->
  <add key="debugMode" value="true" /><!-- Enable Debug information in console, set value to false for disable. Please use this feature is you have much player -->
  <add key="consoleCommand" value="true" /><!-- Enable Server Command Interface, set value to false for disable -->
  <add key="debugPort" value="1172" /><!-- Choose the port for Debug Mode, by default set value to 1172 -->
  <add key="serverPort" value="9339" /><!-- Choose the port for client to connect, by default set value to 9339 -->
  <add key="guiMode" value="true" /><!-- Choose the version of ucs, True for Gui False for Console -->
  <add key="filterFilePath" value="filter.ucs" /><!-- Location of file contain filtered texts -->
  <add key="expertPve" value="true" /><!-- Choose the difficulty of UCS PVE, True for Hard False for Easy -->
  <add key="saveThreadCount" value="5" /><!-- The saving thread count -->
  <add key="programThreadCount" value="5" /><!-- The program thread count -->
  <add key="savingInterval" value="600000" /><!-- The saving interval -->

  <!-- Pro Mode-->
  <add key="adminSpecialMode" value="false" /><!-- Allow admin to bypass maintenance if set to true -->
  <add key="maintenanceProMode" value="true" /><!-- People trying to connect will see a maintenance message if set to true -->
  <add key="maintenanceProMessage" value="Welcome to UCS (No CSV Mod here)" /><!-- People trying to connect will see a custom maintenance message -->
  <!--Run netsh http add urlacl url="http://+:your port/" user=everyone if you choose public API and netsh http delete urlacl url="http://+:your port/" to remove-->
  <add key="apiManagerPro" value="false" /><!-- Enable Api Manager for debugging, set value to false for disable -->
  <add key="ApiKey" value="UCS" />
  <add key="proDebugPort" value="4434" /><!-- Choose the port for Pro Api Manager, by default set value to 1173 -->
  <!-- Pro Mode-->
</appSettings>