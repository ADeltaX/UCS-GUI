<appSettings>
  <add key="startingGems" value="777500"/><!-- Beware, very high value will crash UCS -->
  <add key="startingGold" value="100000000"/><!-- Beware, very high value will crash UCS -->
  <add key="startingElixir" value="500000000"/><!-- Beware, very high value will crash UCS -->
  <add key="startingDarkElixir" value="10000"/><!-- Beware, very high value will crash UCS -->
  <add key="startingTrophies" value="4000"/>
  <add key="startingShieldTime" value="3600"/><!-- Initial shield duration in seconds -->
  <add key="clientVersion" value="7.200"/><!-- Major version dot Client build -->
  <add key="useCustomPatch" value="true"/><!-- Specify if the server is using patches (.csv, .sc, ...) -->
  <add key="patchingServer" value="https://www.smksi2.com/patch/"/><!-- Don't forget the trailing slash -->
  <add key="maintenanceMode" value="false"/><!-- People trying to connect will see a maintenance message if set to true -->
  <add key="maintenanceTimeleft" value="1200"/><!-- Please set the time in seconds like 1200 seconds for 20 minutes -->
  <add key="databaseConnectionName" value="sqliteEntities"/><!-- Chose ucsdbEntities for MySql, and edit ucsdbEntities connection string in ucs.exe.config -->
  <add key="loggingLevel" value="4"/><!-- Enable logging for debugging, set value to 1 for (almost) no log -->
  <add key="apiManager" value="true"/><!-- Enable Api Manager for debugging, set value to false for disable -->
  <add key="oldClientVersion" value="http://ultrapowa.com/"/><!-- Place to redirect old version client and don't forget the trailing slash -->
  <add key="consoleDebug" value="true"/><!-- Enable Debug information in console, set value to false for disable. Please use this feature is you have much player -->
  <add key="consoleCommand" value="true"/><!-- Enable Server Command Interface, set value to false for disable -->
  <add key="debugPort" value="1172"/><!-- Choose the port for Debug Mode, by default set value to 1172 -->
</appSettings>