<appSettings>
  <add key="startingGems" value="777500"/><!-- Beware, very high value will crash UCS -->
  <add key="startingGold" value="100000000"/><!-- Beware, very high value will crash UCS -->
  <add key="startingElixir" value="500000000"/><!-- Beware, very high value will crash UCS -->
  <add key="startingDarkElixir" value="10000"/><!-- Beware, very high value will crash UCS -->
  <add key="startingTrophies" value="4000"/>
  <add key="startingShieldTime" value="3600"/><!-- Initial shield duration in seconds -->
  <add key="startingLevel" value="1" /><!-- User start Level (Beware,9 is the highest starting level) -->
  <add key="startingExperience" value="0" /><!-- Set to random for random - User start experience -->
  <add key="clientVersion" value="7.200"/><!-- Major version dot Client build -->
  <add key="useCustomPatch" value="false"/><!-- Specify if the server is using patches (.csv, .sc, ...) -->
  <add key="patchingServer" value="http://www.ultrapowa.com/patch/"/><!-- Don't forget the trailing slash -->
  <add key="maintenanceMode" value="false"/><!-- People trying to connect will see a maintenance message if set to true -->
  <add key="maintenanceTimeleft" value="1200" /><!-- Please set the time in seconds like 1200 seconds for 20 minutes -->
  <add key="databaseConnectionName" value="sqliteEntities"/><!-- Choose ucsdbEntities for MySql, and edit ucsdbEntities connection string in ucs.exe.config -->
  <add key="loggingLevel" value="4"/><!-- Enable logging for debugging, set value to 1 for (almost) no log -->
  <add key="memoryCleanInterval" value="2500"/><!-- Cleans unused UCS memory after a certain interval in millisecs (2500ms = 2,5s) -->
  <add key="guiMode" value="true"/><!-- Start UCS as GUI (true) or as Console (false). -->
  <add key="proDebugPort" value ="4434"/><!-- Choose the port for Pro Api Manager, by default set value to 1173 -->
  <add key="ApiKey" value="shard" />
  <add key="autoStartServer" value="true" /> <!-- AutoStart the server -->
  <add key="ServerCapacityMode" value="false"/><!-- Enable Server Capacity or disable it -->
  <add key="ServerCapacity" value="1000"/><!-- The Amount of players which are allowed to play -->
  <add key="ServerFullMessage" value="The Server is currently full! Please try again later."/><!-- The Message which should be displayed if the server is full -->
</appSettings>

